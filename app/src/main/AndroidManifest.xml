<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.appctct">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/ctct"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".View.Test.MainActivity"></activity>
        <activity android:name=".View.Login.Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Login.Register_Activity" />
        <activity android:name=".View.Test.PDFReaderActivity" />
        <activity android:name=".View.Setting.ChangePasswordActivity"></activity>
        <activity android:name=".View.Setting.UpdatePhoneNumberActivity">

        </activity>
        <activity android:name=".View.Setting.ForgotPasswordActivity">

        </activity>
    </application>

</manifest>